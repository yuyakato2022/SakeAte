<h3>編集ページ</h3>

<%= form_with model: @user, url: public_user_path, method: :patch do |f| %>
  <p><%= f.file_field :profile_image, placeholder:"プロフィール画像", accept: "image/*" %></p>
  <P><%= f.label :name,"名前" %></P>
  <p><%= f.text_field :name %></p>
  <P><%= f.label :email,"email" %></P>
  <p><%= f.text_field :email %></p>
  <%= f.submit "変更を保存" %>
<% end %>

<%= link_to '退会ページ', public_users_quit_path %>